rankhospital <- function(state, outcome, num = "best") {
data <- read.csv("outcome-of-care-measures.csv", colClasses = "character")
outcome_map <- c("heart attack" = 11, "heart failure" = 17, "pneumonia" = 23)
if (!state %in% unique(data$State)) stop("invalid state")
if (!outcome %in% names(outcome_map)) stop("invalid outcome")
col <- outcome_map[outcome]
data[, col] <- as.numeric(data[, col])
data <- data[data$State == state, ]
data <- data[!is.na(data[, col]), ]
data <- data[order(data[, col], data$Hospital.Name), ]
if (num == "best") num <- 1
else if (num == "worst") num <- nrow(data)
data$Hospital.Name[num]
}
